<%- include("../partials/navbar.ejs") %>
<%- include("../partials/message.ejs") %>

<form method="post" action="/backend/user/loginUser">
  <div class="mb-2">
    <label for="emailId">Email Id</label>
    <input 
    type="email" 
    placeholder="Enter Email"
    class="form-control"
    id="emailId"
    name="emailId"

    >
  </div>
  <div class="mb-2">
    <label for="password">Password</label>
    <input 
    type="password" 
    placeholder="Enter password"
    class="form-control"
    id="password"
    name="password"
    
    >
  </div>
  <input type="submit" value="login">
</form>
<form action="/user/resetUserPassword" method="get">
  <button type="submit">Reset User Password</button>
</form>
<form action="/user/register" method="get">
  <button type="submit">registerUser</button>
  </form>

